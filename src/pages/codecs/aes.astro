---
import Layout from "../../layouts/Layout.astro";
import AesTool from "../../components/tools/AesTool.svelte";
import { getToolByPath } from "../../data/tools";

const tool = getToolByPath("/codecs/aes");
---

<Layout
  title="AES Encrypt / Decrypt - AES-256-CBC, CTR, GCM"
  description={tool?.description}
  keywords={tool?.keywords}
  currentPath="/codecs/aes"
>
  <h1 slot="navbar-title">AES Encrypt / Decrypt</h1>
  <AesTool client:load />
</Layout>
