---
import Layout from "../../layouts/Layout.astro";
import ImageCompressorTool from "../../components/tools/ImageCompressorTool.svelte";
import { getToolByPath } from "../../data/tools";

const tool = getToolByPath("/graphics/compressor");
---

<Layout title="Image Compressor" description={tool?.description} keywords={tool?.keywords} currentPath="/graphics/compressor">
  <h1 slot="navbar-title">Image Compressor</h1>
  <ImageCompressorTool client:load />
</Layout>
